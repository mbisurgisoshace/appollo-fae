      "optional": true
    },
    {
      "name": "children",
      "type": "React.ReactNode",
      "optional": false
    },
    {
      "name": "layoutDirection",
      "type": "LayoutDirection",
      "optional": true,
      "defaultValue": "column"
    }
  ],
  "ui": "section.xml"
}
