{
  "component_name": "Section",
  "stylesheet_classes": [
    {
      "selector": ".Section",
      "attributes": {
        "display": "flex"
      }
    }
  ],
  "types": [
    {
      "name": "LayoutDirection",
      "values": ["row", "column"]
    }
  ],
  "props": [
    {
      "name": "style",
